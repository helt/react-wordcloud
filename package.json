{
  "name": "@inode/react-wordcloud",
  "version": "1.3.0",
  "description": "Simple React + D3 wordcloud component with powerful features.",
  "license": "MIT",
  "repository": "https://git.igd-r.fraunhofer.de/inode/react-wordcloud",
  "bugs": "https://github.com/chrisrzhou/react-wordcloud/issues",
  "publishConfig": {
    "registry": "https://git.igd-r.fraunhofer.de/api/v4/projects/1817/packages/npm/",
    "@inode:registry": "https://git.igd-r.fraunhofer.de/api/v4/projects/1817/packages/npm/",
    "access": "restricted"
  },
  "author": "Chris Zhou <chrisrzhou@pm.me> (https://chrisrzhou.io)",
  "contributors": [
    {
      "name": "Hendrik LÃ¼cke-Tieke",
      "email": "hendrik.luecke-tieke@igd.fraunhofer.de"
    }
  ],
  "keywords": [
    "react",
    "wordcloud",
    "visualization",
    "d3"
  ],
  "scripts": {
    "build": "microbundle --jsx React.createElement",
    "build:docs": "docz build",
    "build:tsc": "tsc",
    "clean": "rimraf dist",
    "clean:docs": "rimraf .docz",
    "docs": "docz dev",
    "lint": "run-s lint:fix build:tsc",
    "lint:fix": "xo --fix src/ docs/",
    "prepare": "run-s clean build"
  },
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "source": "index.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "types/index.d.ts"
  ],
  "dependencies": {
    "d3-array": "^2.5.0",
    "d3-cloud": "^1.2.5",
    "d3-dispatch": "^1.0.6",
    "d3-scale": "^3.2.1",
    "d3-scale-chromatic": "^1.5.0",
    "d3-selection": "^1.4.2",
    "d3-transition": "^1.3.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "resize-observer-polyfill": "^1.5.1",
    "seedrandom": "^3.0.5",
    "tippy.js": "^6.2.6"
  },
  "devDependencies": {
    "@types/d3-array": "^2.0.0",
    "@types/d3-cloud": "^1.2.3",
    "@types/d3-scale": "^2.2.0",
    "@types/d3-scale-chromatic": "^1.5.0",
    "@types/d3-selection": "^1.4.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/seedrandom": "^2.4.28",
    "docz": "^2.3.1",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-config-xo-typescript": "^0.31.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.5",
    "microbundle": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3",
    "xo": "^0.32.1"
  },
  "peerDependencies": {
    "react": "^16.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint:fix"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "useTabs": false
  },
  "xo": {
    "prettier": true,
    "env": [
      "browser"
    ],
    "extends": [
      "xo-react"
    ],
    "rules": {
      "@typescript-eslint/prefer-readonly-parameter-types": "off",
      "import/no-anonymous-default-export": "off",
      "import/no-unassigned-import": "off",
      "react/function-component-definition": "off",
      "react/prop-types": "off",
      "unicorn/no-fn-reference-in-iterator": "off"
    }
  }
}
